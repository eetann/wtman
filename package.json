{
  "name": "wtman",
  "version": "0.1.0",
  "author": "eetann",
  "description": "A CLI tool that manage Git worktree",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eetann/wtman.git"
  },
  "homepage": "https://github.com/eetann/wtman",
  "bugs": {
    "url": "https://github.com/eetann/wtman/issues"
  },
  "keywords": [
    "worktree",
    "cli",
    "command-line"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "wtman": "./dist/index.js"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "biome check",
    "format": "biome format --write",
    "typecheck": "tsgo --noEmit",
    "release": "bun run lint && bun run typecheck && bun run test && bun run build && bumpp",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/bun": "latest",
    "@typescript/native-preview": "^7.0.0-dev.20251201.1",
    "bumpp": "^10.3.2",
    "tsdown": "^0.17.0-beta.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@inquirer/core": "^11.1.0",
    "@inquirer/prompts": "^8.1.0",
    "@inquirer/search": "^4.0.3",
    "confbox": "^0.2.2",
    "console-table-printer": "^2.15.0",
    "defu": "^6.1.4",
    "gunshi": "v0.27.0-beta.3",
    "valibot": "^1.2.0"
  }
}
